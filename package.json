{
  "name": "leaflet-pkk",
  "version": "0.1.0",
  "description": "Public cadastral map (pkk5.rosreestr.ru) plugin for Leaflet",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.com)",
  "license": "BSD-2-Clause",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
