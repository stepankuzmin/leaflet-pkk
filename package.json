    "deploy": "gh-pages -d dist",
    "postpublish": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "gh-pages": "2.1.1",
    "leaflet": "1.5.1",
    "parcel-bundler": "1.12.3",
    "standard-version": "8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "homepage": "https://github.com/stepankuzmin/leaflet-pkk#readme",
  "bugs": {
    "url": "https://github.com/stepankuzmin/leaflet-pkk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stepankuzmin/leaflet-pkk.git"
  },
  "keywords": [
    "leaflet",
    "leaflet-plugin",
    "rosreestr",
    "cadastre",
    "pkk",
    "pkk5"
